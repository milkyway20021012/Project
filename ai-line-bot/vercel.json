{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/app.py"
        },
        {
            "src": "/test",
            "dest": "/app.py"
        },
        {
            "src": "/health",
            "dest": "/app.py"
        },
        {
            "src": "/callback",
            "dest": "/app.py"
        },
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ]
}